#!/bin/execlineb -S0
/bin/s6-fdholder-retrieve -D /var/run/s6/fdholderd-socket init-stdout
/bin/fdmove -c 0 1
/bin/s6-fdholder-retrieve -D /var/run/s6/fdholderd-socket init-stderr
/bin/fdmove -c 0 2

/bin/s6-fdholder-retrieve -D /var/run/s6/fdholderd-socket init-stdin

/bin/exec --
/bin/s6-setsid -g
$@
